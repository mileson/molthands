{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "prisma db push --skip-generate && prisma generate && next build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["hkg1"],
  "rewrites": [
    { "source": "/api/v1/:path*", "destination": "/api/:path*" }
  ],
  "crons": [
    {
      "path": "/api/cron/timeout",
      "schedule": "0 0 * * *"
    }
  ]
}
